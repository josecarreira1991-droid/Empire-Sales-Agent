{
  "gateway": {
    "address": "127.0.0.1",
    "port": 3000,
    "authentication": {
      "mode": "token"
    }
  },
  "models": {
    "providers": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKey": "${DEEPSEEK_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "deepseek-chat",
            "name": "DeepSeek V3 Chat",
            "reasoning": false,
            "inputTypes": ["text"],
            "contextWindow": 128000,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-reasoner",
            "name": "DeepSeek R1 Reasoner",
            "reasoning": true,
            "inputTypes": ["text"],
            "contextWindow": 128000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agent": {
    "model": "deepseek/deepseek-reasoner"
  },
  "agents": {
    "defaults": {
      "heartbeat": {
        "every": "30m",
        "model": "deepseek/deepseek-chat",
        "target": "last",
        "activeHours": {
          "start": "08:00",
          "end": "22:00"
        }
      },
      "models": {
        "allowlist": [
          "deepseek/deepseek-chat",
          "deepseek/deepseek-reasoner"
        ]
      }
    }
  },
  "channels": {
    "whatsapp": {
      "enabled": true,
      "selfChatMode": true,
      "dmPolicy": "allowlist",
      "allowFrom": ["+1OWNER_PHONE_HERE"]
    }
  },
  "plugins": {
    "entries": {
      "voice-call": {
        "enabled": true,
        "config": {
          "provider": "twilio",
          "fromNumber": "${TWILIO_PHONE_NUMBER}",
          "twilio": {
            "accountSid": "${TWILIO_ACCOUNT_SID}",
            "authToken": "${TWILIO_AUTH_TOKEN}"
          },
          "serve": {
            "port": 3334,
            "path": "/voice/webhook"
          },
          "publicUrl": "https://${DOMAIN}/voice/webhook",
          "webhookSecurity": {
            "allowedHosts": ["${DOMAIN}"]
          },
          "outbound": {
            "defaultMode": "notify"
          },
          "streaming": {
            "enabled": true,
            "streamPath": "/voice/stream"
          }
        }
      }
    }
  },
  "messages": {
    "tts": {
      "auto": true,
      "provider": "elevenlabs",
      "elevenlabs": {
        "apiKey": "${ELEVENLABS_API_KEY}",
        "baseUrl": "https://api.elevenlabs.io",
        "voiceId": "${ELEVENLABS_VOICE_ID}",
        "modelId": "eleven_multilingual_v2",
        "languageCode": "en",
        "voiceSettings": {
          "stability": 0.45,
          "similarityBoost": 0.80,
          "style": 0.15,
          "useSpeakerBoost": true,
          "speed": 1.05
        }
      }
    }
  },
  "webhooks": {
    "enabled": true,
    "token": "${WEBHOOK_SECRET_TOKEN}",
    "path": "/hooks/agent"
  },
  "skills": {
    "allowBundled": true
  }
}
